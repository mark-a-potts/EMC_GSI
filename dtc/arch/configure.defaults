###########################################################
#ARCH	AIX 64-bit  #distributed memory parallel (dmpar,optimized)
#
COREDIR        =  CONFIGURE_CORE_PATH
INC_DIR        =  $(COREDIR)/include
SFC            =  xlf_r 
SF90           =  xlf90  -qfree
SCC            =  xlc_r
INC_FLAGS      =  -qmoddir=$(INC_DIR) -I $(INC_DIR) -I CONFIGURE_NETCDF_PATH/include
FFLAGS_i4r4    =  -qintsize=4 -qrealsize=4
FFLAGS_i4r8    =  -qintsize=4 -qrealsize=8
FFLAGS_i8r8    =  -qintsize=8 -qrealsize=8
FFLAGS_DEFAULT =  -q64 -qstrict -qarch=auto
FFLAGS_DEBUG   =  -O0 -qfullpath -qdbg -qsource -qcheck
FFLAGS_OPT     =  -O3
FFLAGS         =  -O3 $(FFLAGS_DEFAULT) -qmaxmem=-1 $(INC_FLAGS) -DIBM4
LDFLAGS        =  
# Library build flags
FFLAGS_BACIO   =  -O3 $(FFLAGS_DEFAULT) -qnosave
ARFLAGS_BACIO  =  -X64
FFLAGS_BUFR    =  -O4 $(FFLAGS_DEFAULT) $(FFLAGS_i4r8) -qnosave -qsource
CFLAGS_BUFR    =  -O3 -q64
ARFLAGS_BUFR   =  -X64
FFLAGS_CLOUD   =  -O3 $(FFLAGS_DEFAULT)
FFLAGS_CRTM    =  -O3 $(FFLAGS_DEFAULT) -qessl -NS32768
LFLAGS_CRTM    =
FFLAGS_GFSIO   =  -O3 -qnosave $(FFLAGS_i4r4) -DIBM4
ARFLAGS_GFSIO  =  -X64
FFLAGS_SFCIO   =  -O -qnosave $(FFLAGS_i4r4)
FFLAGS_SIGIO   =  -O3 -qnosave $(FFLAGS_i4r4)
ARFLAGS_SIGIO  =  -X64
FFLAGS_SP      =  $(FFLAGS) $(FFLAGS_i4r8)
FFLAGS_W3      =  $(FFLAGS) 
#
CPP            =  /usr/ccs/lib/cpp 
CPP_FLAGS      =  -C -D_REAL8_ -DWRF -DIBM8 -Dibm_sp -I/usr/lpp/ppe.poe/include/thread
CPP_F90FLAGS   =  

DM_FC          =  mpxlf_r  
DM_F90         =  mpxlf90_r  -qfree
DM_CC          =  mpcc_r

FC             =  CONFIGURE_FC
F90            =  CONFIGURE_F90
CC             =  CONFIGURE_CC

CFLAGS         =  -I ./ -DFortranByte=char -DFortranInt=int -DFortranLlong='long long' -DIBM4 -DVERBOSE -q64
CFLAGS2        =  $(CFLAGS)

MYLIBsys       =  -lessl_r -lmass -bdatapsize:64K -bstackpsize:64K
NETCDF_PATH    = 


