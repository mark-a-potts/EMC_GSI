create_markers_multilevel   : create_markers_multilevel.o stringmod.o precmod.o 
	ncepxlf90 -o create_markers_multilevel create_markers_multilevel.o stringmod.o precmod.o -L /nwprod/lib -l bufr_4_64

precmod.o    : precmod.f90
	ncepxlf90 -c precmod.f90

stringmod.o    : stringmod.f90
	ncepxlf90 -c stringmod.f90

create_markers_multilevel.o  : create_markers_multilevel.f90 stringmod.f90 precmod.f90
	ncepxlf90 -c create_markers_multilevel.f90 -L /nwprod/lib -l bufr_4_64
