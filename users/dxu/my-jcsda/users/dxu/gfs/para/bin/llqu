#!/bin/csh -f
#set echo
if( $#argv < 1 ) then
  setenv U ${USER}
else
  setenv U $1
endif

setenv tmpdir /scratch2/portfolios/NCEPDEV/ptmp/$U/llqu_out                   
if ( ! -s $tmpdir ) then 
 mkdir -p $tmpdir 
endif

setenv out1 $tmpdir/tmp1$$ 
if ( -s $out1 ) then 
 setenv out1 ${out1}$$ 
endif
setenv out2 $tmpdir/tmp2$$ 
if ( -s $out2 ) then 
  setenv out2 ${out2}$$ 
endif

qstat -au ${U} | awk '/^[0-9]*/{sub(/.bqs1.[a-z]*/,""); printf("%s%4d %s\n",$1,$7,$10)}' | grep "fai " > $out1           

foreach j ( `awk '{print $1}' $out1 ` )
 qstat -f `echo $j |cut -c 1-8` -1 | awk '/Job_Name/{sub(/start_time = /,"");print $0}' >> $out2
end
echo $out1 $out2   

paste $out1 $out2 | sed -e 's/.dayfile//; s#/scratc.*${U}/##'

 
