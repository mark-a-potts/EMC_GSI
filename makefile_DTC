#SHELL=/bin/sh
#  

include ./configure.gsi


SUBDIRS = lib src

all: $(SUBDIRS)
	@for dir in $(SUBDIRS); do \
           ( cd $$dir; echo "Making $@ in `pwd`" ; make -f makefile_DTC ); \
        done

clean: $(SUBDIRS)
	echo "subdirs is: $(SUBDIRS)"
	@for dir in $(SUBDIRS); do \
           ( cd $$dir; echo "Making $@ in `pwd`" ; \
                make -f makefile_DTC $@) ; \
        done
